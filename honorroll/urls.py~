from django.conf.urls import patterns, include, url
HonoreeDelete
from django.contrib import admin
admin.autodiscover()

urlpatterns = patterns('',
    # Examples:
    url(r'^$', 'honorroll.views.home', name='home'),
	url(r'^about/$', 'honorroll.views.about', name='about'),
    url(r'^test/$', ManageHonorView.as_view(), name='honor'),
    url(r'^test2/$', ManageHonoreeView.as_view(), name='honoree'),
    url(r'^admin/', include(admin.site.urls)),
	url(r'organization/$', OrganizationListView.as_view(), name='organization_list'),
    url(r'organization/view/$', OrganizationView.as_view(), name='organization_view'),
    url(r'organization/add/$', OrganizationCreate.as_view(), name='organization_add'),
    url(r'organization/(?P<pk>[0-9]+)/$', OrganizationUpdate.as_view(), name='organization_update'),
    url(r'organization/(?P<pk>[0-9]+)/delete/$', OrganizationDelete.as_view(), name='organization_delete'),
    url(r'honoree/view/$', HonoreeView.as_view(), name='honoree_view'),
    url(r'honoree/add/$', HonoreeCreate.as_view(), name='honoree_add'),
    url(r'honoree/(?P<pk>[0-9]+)/$', HonoreeUpdate.as_view(), name='honoree_update'),
    url(r'honoree/(?P<pk>[0-9]+)/delete/$', HonoreeDelete.as_view(), name='honoree_delete'),
    url(r'app/', include('hr_app.urls'))
)
